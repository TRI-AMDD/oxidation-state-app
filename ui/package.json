            "pre-commit": "lint-staged && npm run lint"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier --write"
        ]
    },
    "devDependencies": {
        "@types/react-router-dom": "^5.3.3",
